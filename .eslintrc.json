{
   "env": {
      "browser": true,
      "node": true
   },
   "rules": {
      "camelcase": "off",
      "no-fallthrough": "off",
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
   },
   "globals": {
      "WB_wombat_location": true
   },
   "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
   }
}
